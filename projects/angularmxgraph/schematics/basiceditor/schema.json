{
  "$schema": "http://json-schema.org/schema",
  "id": "BasicEditorSchematic",
  "title": "mxGraph Basic Editor",
  "type": "object",
  "properties": {
    "path": {
      "type": "string",
      "format": "path",
      "description": "The path to create the basic editor component.",
      "visible": false
    },
    "project": {
      "type": "string",
      "description": "The name of the project to create the component in.",
      "$default": {
        "$source": "projectName"
      }
    },
    "name": {
      "type": "string",
      "description": "The name of the component",
      "default": "basiceditor"
    }
   },
  "required": []
}
